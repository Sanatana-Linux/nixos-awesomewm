#!/bin/sh

LUAROCKS_SYSCONFDIR='/home/tlh/.config/awesome/libraries/luajit/etc/luarocks' exec '/home/tlh/.config/awesome/libraries/luajit/bin/lua' -e 'package.path="/home/tlh/.config/awesome/libraries/luajit/share/lua/5.1/?.lua;/home/tlh/.config/awesome/libraries/luajit/share/lua/5.1/?/init.lua;"..package.path;package.cpath="/home/tlh/.config/awesome/libraries/luajit/lib/lua/5.1/?.so;"..package.cpath;local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("ldoc","1.4.6-2")' '/home/tlh/.config/awesome/libraries/luajit/lib/luarocks/rocks-5.1/ldoc/1.4.6-2/bin/ldoc' "$@"
